{
  "name": "stylelint-plugin-dialtone",
  "targets": {
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm publish --filter ./packages/stylelint-plugin-dialtone"
      }
    },
    "release": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm semantic-release-plus --extends ./packages/stylelint-plugin-dialtone/release.config.cjs",
        "parallel": false
      }
    }
  }
}
