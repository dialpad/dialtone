{
  "name": "stylelint-plugin-dialtone",
  "targets": {
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm publish --filter ./packages/stylelint-plugin-dialtone",
        "parallel": false
      }
    },
    "release-github": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm semantic-release-plus --extends ./packages/stylelint-plugin-dialtone/release-ci.config.cjs",
        "parallel": false
      }
    },
    "release-local": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm semantic-release-plus --skip-tag --extends ./packages/stylelint-plugin-dialtone/release-local.config.cjs",
        "parallel": false
      }
    }
  }
}
