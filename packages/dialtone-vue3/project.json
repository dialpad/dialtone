{
  "name": "dialtone-vue3",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "options": { "script": "build" }
    },
    "build-docs": {
      "executor": "nx:run-script",
      "options": { "script": "storybook:build" }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm publish --filter --dry-run ./packages/dialtone-vue3"
      }
    },
    "release": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm semantic-release-plus --extends ./packages/dialtone-vue3/release.config.cjs",
        "parallel": false
      }
    },
    "test": {
      "executor": "nx:run-script",
      "options": { "script": "test" }
    }
  }
}
