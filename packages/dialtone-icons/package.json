    },
    {
      "name": "Julio Ortega",
      "email": "julio.ortega@dialpad.com",
      "url": "https://github.com/juliodialpad"
    },
    {
      "name": "Ignacio Ropolo",
      "email": "ignacio.ropolo@dialpad.com",
      "url": "https://github.com/iropolo"
    },
    {
      "name": "Nina Repetto",
      "email": "nina.repetto@dialpad.com",
      "url": "https://github.com/ninamarina"
    }
  ],
  "keywords": [
    "Dialpad",
