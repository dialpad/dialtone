{
  "name": "@dialpad/dialtone-icons",
  "version": "4.15.1-alpha.1",
  "description": "Dialtone icon library",
  "files": [
    "dist",
    "vue2/dist",
    "vue3/dist"
  ],
  "exports": {
    "./vue2": {
      "import": "./vue2/dist/dialtone-icons.js",
      "require": "./vue2/dist/dialtone-icons.cjs"
    },
    "./vue2/*": {
      "import": "./vue2/dist/components/*.js",
      "require": "./vue2/dist/components/*.cjs"
    },
    "./vue3": {
      "import": "./vue3/dist/dialtone-icons.js",
      "require": "./vue3/dist/dialtone-icons.cjs"
    },
    "./vue3/*": {
      "import": "./vue3/dist/components/*.js",
      "require": "./vue3/dist/components/*.cjs"
    },
    "./icons.json": "./dist/icons.json",
    "./illustrations.json": "./dist/illustrations.json",
    "./keywords-icons.json": "./dist/keywords-icons.json",
    "./keywords-illustrations.json": "./dist/keywords-illustrations.json",
    "./*": "./dist/svg/*"
  },
  "devDependencies": {
    "del": "^6.1.1",
    "glob": "^10.3.10",
    "gulp": "^4.0.2",
    "gulp-json-format": "^2.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-svgmin": "^4.1.0",
    "lodash": "^4.17.21"
  },
  "bugs": {
    "email": "dialtone@dialpad.com"
  },
  "contributors": [
