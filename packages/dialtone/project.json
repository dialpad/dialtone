        "command": "npx semantic-release-plus --extends ./packages/dialtone/release-ci.config.cjs",
        "parallel": false
      }
    },
    "start": {
      "executor": "nx:run-script",
      "outputs": [],
      "dependsOn": [
        {
          "projects": ["dialtone-tokens"],
          "target": "build",
          "params": "ignore"
        }
      ],
      "options": {
        "script": "start"
      }
