{
  "name": "dialtone",
  "targets": {
    "release-local": {
      "executor": "nx:run-commands",
      "outputs": [],
      "dependsOn": [
        {
          "projects": ["dialtone-tokens"],
          "target": "build",
          "params": "ignore"
        }
      ],
      "options": {
        "command": "npx semantic-release-plus --no-ci --extends ./packages/dialtone/release-local.config.cjs",
