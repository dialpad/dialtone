{
  "name": "dialtone-emojis",
  "targets": {
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm publish --filter ./packages/dialtone-emojis"
      }
    },
    "release-github": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm semantic-release-plus --extends ./packages/dialtone-emojis/release-ci.config.cjs"
      }
    },
    "release-local": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm semantic-release-plus --no-ci --extends ./packages/dialtone-emojis/release-local.config.cjs"
      }
    }
  }
}
