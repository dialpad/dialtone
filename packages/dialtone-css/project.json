{
  "name": "dialtone-css",
  "targets": {
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm publish --filter ./packages/dialtone-css"
      }
    },
    "release-github": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm semantic-release-plus --extends ./packages/dialtone-css/release-ci.config.cjs"
      }
    },
    "release-local": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm semantic-release-plus --skip-tag --extends ./packages/dialtone-css/release-local.config.cjs"
      }
    },
    "start": {
      "executor": "nx:run-script",
      "options": { "script": "start" }
    }
  }
}
