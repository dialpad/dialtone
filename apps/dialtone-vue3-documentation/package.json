{
  "name": "@dialpad/dialtone-vue3-documentation",
  "version": "1.0.0",
  "description": "Dialtone Vue library documentation site",
  "scripts": {
    "lint:fix": "pnpm run lint --fix",
    "lint": "markdownlint '**/*.mdx'",
    "start": "STORYBOOK_ENV=development storybook dev -p 9010",
    "clean": "rm -rf ./storybook-static",
    "build": "pnpm run clean && pnpm run build:documentation:functions && STORYBOOK_ENV=production storybook build",
    "build:documentation:functions": "pnpm exec typedoc --plugin typedoc-plugin-markdown",
    "build:docs-only": "pnpm run build --docs",
    "test:a11y": "test-storybook --url http://localhost:9010/",
    "test:visual": "pnpm exec percy storybook --disable-cache --config ../../percy.config.cjs --verbose ./storybook-static"
  },
