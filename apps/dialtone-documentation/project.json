{
  "name": "dialtone-documentation",
  "targets": {
    "build-docs": {
      "executor": "nx:run-script",
      "dependsOn": [
        {
          "projects": [
            "dialtone",
            "dialtone-icons",
            "dialtone-vue3"
          ],
          "target": "build",
          "params": "ignore"
        }
      ],
      "outputs": [],
      "options": {
        "script": "build",
        "parallel": true
      }
    },
    "lint-staged": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm lint-staged",
        "parallel": true
      }
    },
    "start": {
      "executor": "nx:run-script",
      "outputs": [],
      "dependsOn": [
        {
          "projects": ["dialtone-vue3", "dialtone-icons"],
          "target": "build",
          "params": "ignore"
        }
      ],
      "options": {
        "script": "start",
        "parallel": true
      }
    }
  }
}
