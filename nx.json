{
  "extends": "nx/presets/npm.json",
  "affected": {
    "defaultBase": "production"
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"]
    },
    "test": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "release": {
      "cache": false,
      "dependsOn": ["build", "^build"]
    }
  },
  "installation": {
    "version": "17.0.2"
  }
}
