    "@tiptap/vue-2": {
      "optional": true
    },
    "@tiptap/vue-3": {
      "optional": true
    }
  },
  "homepage": "https://dialtone.dialpad.com",
  "keywords": [
    "Dialpad",
    "Dialtone",
    "Dialtone Vue",
    "Dialtone Icons",
    "Dialtone Tokens",
    "Design System",
    "Components",
    "Component Library"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dialpad/dialtone.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "nx": {
    "includedScripts": []
  }
}
