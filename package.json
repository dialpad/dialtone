{
  "name": "@dialpad/dialtone-vue",
  "version": "2.36.0",
  "description": "Vue component library for Dialpad's design system Dialtone",
  "keywords": [
    "Dialpad",
    "Dialtone",
    "Dialtone Vue",
    "Design System",
    "Components",
    "Component Library",
    "Vue"
  ],
  "homepage": "https://vue.dialpad.design",
  "bugs": {
    "email" : "dialtone@dialpad.com"
  },
  "license": "MIT",
  "contributors": [
    {"name":"Brad Paugh","email":"brad.paugh@dialpad.com","url":"https://github.com/braddialpad"},
    {"name":"Francis Rupert","email":"francis.rupert@dialpad.com","url":"https://github.com/francisrupert"},
    {"name":"Julio Ortega","email":"julio.ortega@dialpad.com","url":"https://github.com/juliodialpad"},
    {"name":"Jose Silva","email":"jose.silva@dialpad.com","url":"https://github.com/josedialpad"}
  ],
  "files": [
    "dist/*.js",
    "dist/component-documentation.json"
  ],
  "repository": "git@github.com:dialpad/dialtone-vue.git",
  "scripts": {
    "build": "npm run clean && npm run build-main && npm run build-emoji && npm run build:documentation",
    "build-main": "vue-cli-service build --target lib --name dialtone-vue --no-clean index.js",
    "build-emoji": "vue-cli-service build --target lib --name emoji --no-clean emoji.js",
    "build:documentation": "node scripts/build-docs.js",
    "clean": "rm -rf ./dist",
    "pretest": "npm run clean",
    "lint": "run-s lint:code lint:docs",
    "build:wc": "vue-cli-service build --target wc-async --name dialtone-vue 'components/**/*[!.story]*.vue'",
    "lint-staged:code": "eslint --fix",
    "lint-staged:docs": "markdownlint --fix",
    "lint:code": "esplint ./",
    "lint:docs": "markdownlint '**/*.mdx'",
    "prepublishOnly": "npm run build",
    "release": "semantic-release --no-ci --extends ./release-local.config.js",
    "start": "npm run storybook",
    "storybook": "npm run --prefix storybook storybook",
    "storybook:build": "npm run --prefix storybook build",
    "storybook:install": "npm install --prefix storybook",
    "test": "vue-cli-service test:unit --timeout 100000 --require source-map-support/register --colors components/**/*.test.js recipes/**/**/*.test.js",
    "test:one": "vue-cli-service test:unit --timeout 100000 --require source-map-support/register --colors",
    "test:watch": "vue-cli-service test:unit --watch --timeout 100000 --require source-map-support/register --colors components/**/*.test.js recipes/**/**/*.test.js",
    "storybook:a11y-test": "npm run --prefix storybook storybook-a11y-test",
    "install:all": "npm install && npm install --prefix storybook && npm install --prefix generator-dialtone-vue"
  },
  "exports": {
    ".": "./dist/dialtone-vue.common.js",
    "./emoji": "./dist/emoji.common.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "./scripts/commit-msg"
  },
  "volta": {
    "node": "16"
  },
  "dependencies": {
    "@dialpad/dialtone-icons": "0.0.8",
    "@linusborg/vue-simple-portal": "^0.1.5",
    "emoji-regex": "^10.1.0",
    "emoji-toolkit": "^6.6.0",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@dialpad/dialtone": "^7.4.1",
    "@percy/cli": "1.10.2",
    "@percy/storybook": "4.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-unit-mocha": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/test-utils": "1.3",
    "axe-core": "^4.4.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "chai": "^4.3.6",
    "eslint": "8.22",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-vue": "^9.4.0",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "esplint": "^0.10.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "macos-release": "^3.1.0",
    "markdownlint": "^0.26.2",
    "markdownlint-cli": "^0.32.2",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "semantic-release": "^19.0.5",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.21",
    "vue": "^2.7.13",
    "vue-cli-plugin-webpack-bundle-analyzer": "~4.0.0",
    "vue-docgen-api": "^4.52.0",
    "vue-template-compiler": "^2.7.13",
    "yo": "^4.3.0"
  },
  "peerDependencies": {
    "@dialpad/dialtone": ">=7.4",
    "vue": ">=2.6"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 16"
  }
}
